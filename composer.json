{
  "name": "tourze/wechat-work-jssdk-bundle",
  "description": "企业微信JSSDK",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/collections": "^2.3",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-foundation": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/enum-extra": "0.0.*",
    "tourze/json-rpc-core": "^0.0.6",
    "tourze/json-rpc-lock-bundle": "^0.0.6",
    "tourze/http-client-bundle": "0.0.*",
    "tourze/wechat-work-bundle": "0.0.*",
    "nesbot/carbon": "^2.72 || ^3"
  },
  "autoload": {
    "psr-4": {
      "WechatWorkJssdkBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WechatWorkJssdkBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}