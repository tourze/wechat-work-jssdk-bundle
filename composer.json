{
  "name": "tourze/wechat-work-jssdk-bundle",
  "description": "企业微信JSSDK集成包，提供企业微信应用的JavaScript SDK功能",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "doctrine/collections": "^2.3",
    "nesbot/carbon": "^2.72 || ^3",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/routing": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/enum-extra": "1.0.*",
    "tourze/http-client-bundle": "1.0.*",
    "tourze/json-rpc-core": "1.0.*",
    "tourze/json-rpc-lock-bundle": "1.0.*",
    "tourze/symfony-dependency-service-loader": "1.0.*",
    "tourze/symfony-routing-auto-loader-bundle": "1.0.*",
    "tourze/wechat-work-bundle": "1.0.*",
    "tourze/wechat-work-contracts": "1.0.*"
  },
  "require-dev": {
    "doctrine/dbal": "^4.0",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "psr/log": "^3|^2|^1",
    "symfony/cache-contracts": "^3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/lock": "^7.3",
    "symfony/phpunit-bridge": "^7.3",
    "tourze/doctrine-async-insert-bundle": "1.0.*",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-enum": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*",
    "tourze/phpunit-symfony-web-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "WechatWorkJssdkBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WechatWorkJssdkBundle\\Tests\\": "tests"
    }
  }
}